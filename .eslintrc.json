{
  "extends": "next/core-web-vitals",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0
      }
    ],
    "max-len": [
      "error",
      {
        "code": 80,
        "ignoreStrings": true
      }
    ]
  }
}